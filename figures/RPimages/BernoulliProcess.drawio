        <mxCell id="6eMtLj5RxkQClJ1PiOg4-41" value="&lt;font&gt;Bernoulli Process \((t;p)\)&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="1" vertex="1">
          <mxGeometry x="80" y="200" width="190" height="30" as="geometry" />
        </mxCell>
        <mxCell id="6eMtLj5RxkQClJ1PiOg4-43" value="&lt;font&gt;The result of each trial (0 : failure ;1: success):&amp;nbsp;&lt;/font&gt;&lt;div&gt;&lt;font&gt;$$X_i \in \{0,1\} \sim Bernoulli (p), p \in[0,1]$$&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="1" vertex="1">
          <mxGeometry x="425" y="150" width="420" height="30" as="geometry" />
        </mxCell>
        <mxCell id="6eMtLj5RxkQClJ1PiOg4-44" value="&lt;font&gt;The total number of successes in n trials&lt;/font&gt;&lt;div&gt;&lt;font&gt;$$Y_i \in \{0,1,2,\cdots,n\} \sim Binomial (n,p), p \in[0,1], n \in \mathbb{Z}_{&amp;gt;0}$$&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="1" vertex="1">
          <mxGeometry x="425" y="370" width="270" height="30" as="geometry" />
        </mxCell>
        <mxCell id="6eMtLj5RxkQClJ1PiOg4-45" value="&lt;font&gt;The number of trials between \((i-1)^{th}\) and \(i^{th}\) success:&amp;nbsp;&lt;/font&gt;&lt;div&gt;&lt;font&gt;$$N_i \in \{1,2,\cdots\} \sim Geometric (p), p \in[0,1]$$&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="1" vertex="1">
          <mxGeometry x="425" y="520" width="430" height="30" as="geometry" />
        </mxCell>
        <mxCell id="6eMtLj5RxkQClJ1PiOg4-64" value="&lt;font&gt;The number of trials to get the \(k^{th}\) success:&amp;nbsp;&lt;/font&gt;&lt;div&gt;&lt;font&gt;$$W_k \in \{1,2,\cdots\} \sim Negative Binomial(p,k), p \in[0,1] , k \in \mathbb{Z}_{&amp;gt;0}$$&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="1" vertex="1">
