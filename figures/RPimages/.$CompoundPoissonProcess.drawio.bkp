        <mxCell id="-9YXquRkQ-doIjd-wjBv-2" value="&lt;font&gt;The total number of successes until time t&lt;/font&gt;&lt;div&gt;&lt;font&gt;$$Y(t)&amp;nbsp; \in \{0,1,2,\cdots\} \sim poisson (\nu), \nu= \lambda t , \lambda&amp;gt;0,t&amp;gt;0 $$&amp;nbsp;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="1" vertex="1">
          <mxGeometry x="430" y="648" width="420" height="32" as="geometry" />
        </mxCell>
        <mxCell id="-9YXquRkQ-doIjd-wjBv-3" value="&lt;font&gt;The time or distance between \((i-1)^{th}\) and \(i^{th}\) success:&amp;nbsp;&lt;/font&gt;&lt;div&gt;&lt;font&gt;$$T_i \in (0,+\infty) \sim exp (\lambda), \lambda &amp;gt;0 $$&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="1" vertex="1">
          <mxGeometry x="435" y="270" width="415" height="30" as="geometry" />
        </mxCell>
        <mxCell id="-9YXquRkQ-doIjd-wjBv-4" value="&lt;font&gt;The time until obtaining the \(k^{th}\) success:Erlang dist.&amp;nbsp;&lt;/font&gt;&lt;div&gt;&lt;font&gt;$$V_k \in (0,+\infty) \sim \Gamma(k,\lambda), \lambda &amp;gt;0, k = 1,2,\cdots $$&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="1" vertex="1">
          <mxGeometry x="440" y="440" width="420" height="30" as="geometry" />
        </mxCell>
        <mxCell id="-9YXquRkQ-doIjd-wjBv-35" value="" style="group" parent="1" vertex="1" connectable="0">
          <mxGeometry x="850" y="260" width="280" height="125" as="geometry" />
        </mxCell>
        <mxCell id="-9YXquRkQ-doIjd-wjBv-36" value="&lt;font&gt;PDF \(f_{T_i}(t) = \lambda e^{-\lambda t}\)&lt;/font&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="-9YXquRkQ-doIjd-wjBv-35" vertex="1">
          <mxGeometry width="271.25" height="30" as="geometry" />
        </mxCell>
        <mxCell id="-9YXquRkQ-doIjd-wjBv-37" value="&lt;font&gt;CDF \(F_{T_i}(t) =1-e^{-\lambda t}\)&lt;/font&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="-9YXquRkQ-doIjd-wjBv-35" vertex="1">
          <mxGeometry y="40" width="280" height="30" as="geometry" />
        </mxCell>
        <mxCell id="-9YXquRkQ-doIjd-wjBv-38" value="&lt;div&gt;&lt;font&gt;$$\mu_{T_i} = \frac{1}{\lambda}, \sigma_{T_i} = \frac{1}{\lambda}$$&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="-9YXquRkQ-doIjd-wjBv-35" vertex="1">
