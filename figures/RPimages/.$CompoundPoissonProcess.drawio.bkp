        </mxCell>
        <mxCell id="-9YXquRkQ-doIjd-wjBv-43" value="&lt;font style=&quot;font-size: 18px;&quot;&gt;$$\lim_{y \rightarrow \infty, p \rightarrow 0, yp \rightarrow \lambda t} binomial(y,p) = poisson (\lambda t)$$&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" parent="1" vertex="1">
