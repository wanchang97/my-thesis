        <mxCell id="-9YXquRkQ-doIjd-wjBv-40" value="&lt;font&gt;PDF \(f_{V_k}(t)&amp;nbsp;=\frac{\lambda^k t^{k-1}e^{-\lambda t}}{\Gamma(k)} = \frac{\lambda^k t^{k-1}e^{-\lambda t}}{(k-1)!}&amp;nbsp;\)&lt;/font&gt;&lt;div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="-9YXquRkQ-doIjd-wjBv-39" vertex="1">
          <mxGeometry width="290.62499999999994" height="30" as="geometry" />
        </mxCell>
        <mxCell id="-9YXquRkQ-doIjd-wjBv-41" value="&lt;div&gt;&lt;font&gt;$$\mu_{V_k} = \frac{k}{\lambda}, \sigma_{V_k} = \frac{\sqrt{k}}{\lambda}$$&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="-9YXquRkQ-doIjd-wjBv-39" vertex="1">
          <mxGeometry y="69" width="196.875" height="55" as="geometry" />
        </mxCell>
        <mxCell id="-9YXquRkQ-doIjd-wjBv-42" value="&lt;font&gt;CDF&lt;/font&gt;&lt;font style=&quot;background-color: light-dark(#ffffff, var(--ge-dark-color, #121212)); color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;&amp;nbsp;\(F_{V_k}(t) = \frac{\gamma(k,\lambda t)}{\Gamma(k)} = \frac{\int_0^{\lambda t}\tau^{k-1}e^{-\tau}d\tau}{(k-1)!}\)&lt;/font&gt;" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=Times New Roman;fontSize=18;fontColor=default;labelBackgroundColor=default;" parent="-9YXquRkQ-doIjd-wjBv-39" vertex="1">
